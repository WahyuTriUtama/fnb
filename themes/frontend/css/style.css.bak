}
.navbar-brand {
	float: none;
	height: auto;
	z-index: 9999;
	position: relative;
	padding-top: 30px;
	display: inline-block;
}
.navbar-brand img {
	max-height: 120px;
	border-radius: 0 0 6px 6px;
	-moz-border-radius: 0 0 6px 6px;
	-webkit-border-radius: 0 0 6px 6px;
}
.navbar-default {
	background: transparent;
	box-shadow: none;
	border: 0;
}
.nav>li>a {
	padding: 14px;
	font-size: 14px;
	font-family: 'FuturaStd-Light';
	text-transform: uppercase;
}
.nav>li>a:hover {
	color: #C60!important;
}

.dropdown-menu>li>a {
	padding: 7px 10px;
	font-size: 14px;
	font-family: 'FuturaStd-Light';
	min-width: 200px;
}
.dropdown-menu>li > a > .has-submenu {padding: 4px 5px;float: right;}
.dropdown-menu>li .submenu a:hover {text-decoration: none;}
.dropdown-menu>li {position: relative;}
.dropdown-menu>li>hr {
	margin-top: 5px;
	margin-bottom: 5px;
}

.navbar-nav>li>.dropdown-menu {
	left: auto;
	right: 0;
}

.navbar-nav>li:nth-child(1)>.dropdown-menu,
.navbar-nav>li:nth-child(2)>.dropdown-menu,
.navbar-nav>li:nth-child(3)>.dropdown-menu {
	right: auto;
	left: 0;
}

/* submenu */
.dropdown-menu > li > ul.submenu {
	width: 176px;
	padding: 0px;
	margin: 0px;
	background: #ecf0f1;
	position: absolute;
	top: 0px;
	left: 100%;
	display: none;
	-webkit-box-shadow: 0 2px 2px 0 rgba(0,0,0,.3);
	box-shadow: 0 2px 2px 0 rgba(0,0,0,.3);
	z-index: 899;

	border-radius: 6px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;

	border-top: 4px solid #151515;
}

.dropdown-menu > li > ul.submenu ul{left: 180px;top:0px;}
.dropdown-menu > li > ul.submenu li{
	padding: 0px;
	padding: 0px 13px;
	margin: 0px;
	display: block;
	float: none;
	border-bottom: 1px solid rgba(0,0,0,0.1);
	border-top: 1px solid rgba(255,255,255,1);
	line-height: 40px;
	text-transform: none;
	text-transform: uppercase;
	position: relative;
}
.dropdown-menu > li > ul.submenu li:first-child{border-top: none;}
.dropdown-menu > li:hover > ul{display:block;}
.dropdown-menu > li > ul li:hover > ul{display:block;}
.dropdown-menu > li > ul.submenu li a {font-weight: normal; color: #c61e0d;display: block;}
.dropdown-menu > li > ul.submenu li a:hover {color: #000;}

.dropdown-menu > li > ul.submenu li:hover {
	-webkit-box-shadow: inset 0px -60px 132px 13px rgba(0,0,0,0.26);
	-moz-box-shadow: inset 0px -60px 132px 13px rgba(0,0,0,0.26);
	box-shadow: inset 0px -60px 132px 13px rgba(0,0,0,0.26);
} 
.dropdown-menu > li > li:hover > ul{
	display:block;
	-moz-animation: fadeInUp .3s ease-in  ;
	-webkit-animation: fadeInUp .3s ease-in ;
	animation:fadeInUp .3s ease-in ;
}

.dropdown-menu > li > ul li:hover > ul{
	display: block;
	-moz-animation: fadeInRight .3s ease-in;
	-webkit-animation: fadeInRight .3s ease-in;
	animation: fadeInRight .3s ease-in;
	top: -2px;
	position: absolute;
	left: 100%;
	border-top: 1px solid rgba(0,0,0,0.6);
}

.navbar-nav > li > .dropdown-menu > li:hover > ul{
	left: auto;
	right: 100%;
}
.navbar-nav>li:nth-child(1)> .dropdown-menu > li:hover > ul,
.navbar-nav>li:nth-child(2)> .dropdown-menu > li:hover > ul,
.navbar-nav>li:nth-child(3)> .dropdown-menu > li:hover > ul {
	left: 100%;
	right: auto;
}
/* submenu */

