#ibe_services table.wrapper td:nth-child(2) {width: 40%;}
#ibe_services table.wrapper tr td.property {background: transparent!important;padding-bottom: 20px!important;}
#ibe_services h2 {font-size: 22px!important; font-weight: bold!important;}

@media (max-width: 767px) {
	.nav > li > a {
		text-align: right;
		font-weight: bolder;
		padding: 10px 15px;
		border-bottom: 1px solid #ddd;
		font-size: 16px;
	}
	.nav .open>a, .nav .open>a:hover, .nav .open>a:focus {
		border-color: #C60;
	}
	.navbar-default .navbar-nav .open .dropdown-menu>li>a {
		text-align: right;
		background: #f5f5f5;
		border-bottom: 1px dotted #ddd;
		padding: 10px 15px;
		text-transform: uppercase;
	}
	.navbar-default .navbar-nav .open .dropdown-menu>li>a:hover {
		background: #eee;
	}
	.navbar-default .navbar-nav .open .dropdown-menu {
		padding: 0px;
	}
	.navbar-toggle {
		padding: 15px 15px;
		margin-bottom: -5px;
		background: #fff;
	}
	.navbar-default .navbar-toggle .icon-bar {
		background-color: #000;
	}
	.navbar-default .navbar-toggle:hover .icon-bar {
		background-color: #b81c1a;
	}
	.navbar-nav {margin: 0;}
	#imageslideshow {
		/*height: 220px;*/
	}
	#imageslideshow .container {
		padding: 0;
	}
	.carousel-inner>.item>img, .carousel-inner>.item>a>img {
		width: 100%;
	}
	.navbar-header {
		display: inline-block;
		width: 100%;
		height: 60px;
		background: #000;
	}
	.navbar-brand {
		background: #fff;
		display: block;
		text-align: center;
	}
	.widget-addrs {
		padding: 20px 20px;
		color: #fff;
		text-align: center;
	}
	header {margin-bottom: 0;overflow: hidden;}
	.navbar-default .navbar-collapse, .navbar-default .navbar-form {
		background: #eee;
	}

	#ibe_form_checkin, #ibe_form_promocode {
		width: 97%!important;
		display: block!important;
		margin-bottom: 15px!important;
	}

	#ibe_form_submit_check, #ibe_form_submit_cancel {
		width: 45%!important;
		margin-right: 3px!important;
	}

	#widget-info > .text-right {text-align: left!important;}
	#widget-info {margin-bottom: 5px;}
	#mainwraper .well, #mainwraper .panel {border-radius: 0;}
	#widget-news-list ul {padding: 0; margin: 0;}
	#sidebar {padding: 0;}
	footer {font-size: 10px;}  
	#ibe_reservation ul.cal {margin-bottom: 20px!important;}                                                                                                                                                                                                                                                                                                                             
}

@media (max-width: 480px) {
	#ibe_reservation ul.cal {margin-bottom: 20px!important;}
	#ibe_reservation ul.form li {
		float: right!important;
		margin-bottom: 10px;
	}
	#ibe_booking #ibe_booking_bottom {
		display: inline-block;
		border-bottom: 1px solid #000;
		padding-bottom: 20px;
	}
	#ibe_reservation ul.cal li.prices {
		width: 70px!important;
		font-size: 10px;
	}
	#ibe_reservation ul.cal li span.title {font-size: 10px;}
	#ibe_reservation ul.cal li.prices ul li {font-size: 11px!important;}
	#ibe_reservation ul.cal li.calendar {width: 280px;}
	#ibe_reservation ul.cal li.calendar .wrap ul.dates li {
		width: 55px!important;
	}
	#ibe_reservation ul.cal li.calendar .wrap ul.picker li {
		width: 55px!important;
		font-size: 11px!important;
	}
	#ibe_reservation ul.cal li.calendar .wrap ul.dates,
	#ibe_reservation ul.cal li.calendar .wrap ul.picker {
		width: 400px!important;
	}
	#ibe_services_profile {overflow: auto;}
	#ibe_services_profile table.wrapper {width: 680px!important;}
}

@media (max-width: 411px) {
	#ibe_reservation ul.cal li.calendar {width: 230px!important;}
}
@media (max-width: 446px) {
	#ibe_reservation ul.cal li.calendar {width: 225px!important;}
}
@media (max-width: 480px) {
	#ibe_reservation ul.cal li.calendar {width: 240px!important;}
}
@media (min-width: 481px) and (max-width: 767px) {
	#ibe_reservation ul.cal li.prices {
		width: 70px!important;
		font-size: 10px;
	}
	#ibe_reservation ul.cal li span.title {font-size: 10px;}
	#ibe_reservation ul.cal li.prices ul li {font-size: 11px!important;}
	#ibe_reservation ul.cal li.calendar .wrap ul.dates li {
		width: 60px!important;
	}
	#ibe_reservation ul.cal li.calendar .wrap ul.picker li {
		width: 60px!important;
		font-size: 11px!important;
	}
	#ibe_reservation ul.cal li.calendar .wrap ul.dates,
	#ibe_reservation ul.cal li.calendar .wrap ul.picker {
		width: 450px!important;
	}
	#ibe_services_profile {overflow: auto;}
	#ibe_reservation ul.cal li.calendar {max-width: 420px!important;}
	#ibe_reservation ul.cal li.calendar .wrap ul.picker {
		height: 50px!important;
	}
	.cal .calendar, .cal .calendar .wrap {width: auto!important;}
}